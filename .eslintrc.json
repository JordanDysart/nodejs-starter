{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "require-jsdoc": "off",
      "new-cap": "off",
      "object-curly-spacing": "off",
      "comma-dangle": "off",
      "no-unused-vars": "warn",
      "no-undef": "error"
    }
}