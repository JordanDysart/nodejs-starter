{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "google"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "require": "readonly",
      "process": "readonly",
      "env": "readonly",
      "__dirname": "readonly",
      "module": "readonly",
      "exports": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "new-cap": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "no-undef": "error",
    "arrow-parens": "off",
    "max-len": "warn",
    "no-unused-vars": [2, {"args": "all", "argsIgnorePattern": "^unused"}]
  }
}